<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="AnchoredThumbSeekBar">
        <!-- Defines the maximum value the progress can take. -->
        <attr name="max" format="integer" />
        <!-- Defines the default progress value, between 0 and max. -->
        <attr name="progress" format="integer" />
        <attr name="barHeight" format="dimension" />
        <attr name="thumbWidth" format="dimension" />
        <attr name="barOffset" format="dimension" />
        <attr name="elapsedDrawable" format="reference|color" />
        <attr name="remainingDrawable" format="reference|color" />
        <attr name="thumbDrawable" format="reference|color" />
    </declare-styleable>
</resources>

